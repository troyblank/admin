{
    "name": "troyblank-admin",
    "version": "1.0.0",
    "scripts": {
        "start": "yarn clean && yarn watch",
        "clean": "rm -rf dist && rm -rf coverage",
        "watch": "vite",
        "build": "vite build",
        "test": "npm run lint && npm run unit:once",
        "lint": "tsc --noEmit && eslint --max-warnings=0 src --ext .ts,.tsx",
